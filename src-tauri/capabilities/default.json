{
  "identifier": "default",
  "description": "Default capabilities for clarc",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:default",
    "process:default",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "binaries/clarc-core",
          "sidecar": true,
          "args": true
        }
      ]
    }
  ]
}
